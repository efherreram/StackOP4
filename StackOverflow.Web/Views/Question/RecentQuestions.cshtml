@model IEnumerable<StackOverflow.Web.Models.QuestionListModel>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Votes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreationTime)
        </th>
        <th>
            @Html.DisplayName("Preview")
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Views)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <a href="@Url.Action("QuestionDetails","Question",new{id=item.QuestionId})">@Html.DisplayName(item.Title)</a>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Votes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreationTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QuestionPreview)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Views)
        </td>
    </tr>
}

</table>
